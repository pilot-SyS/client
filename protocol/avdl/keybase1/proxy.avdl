@namespace("keybase.1")

protocol Proxy {
  import idl "common.avdl";

  enum ProxyType {
    No_Proxy_0,
    HTTP_Connect_1,
    Socks_2
  }

  record ProxyData {
    string addressWithPort;
    ProxyType proxyType;
    boolean certPinning;
  }

  void setProxyData(ProxyData proxyData);

  ProxyData getProxyData();
}
